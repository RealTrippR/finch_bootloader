{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "StartQemu",
            "type": "shell",
            "command": "echo 'QEMU started';  qemu-system-i386 -s -S bin/os.bin",
            "isBackground": true,
            "problemMatcher": [
            {
                "pattern": [
                    {
                        "regexp": ".",
                        "file": 1,
                        "location": 2,
                        "message": 3
                    }
                    ],
                    "background": {
                    "activeOnStart": true,
                    "beginsPattern": ".",
                    "endsPattern": "QEMU started",
                    }
                }
            ]
        }
    ]
}